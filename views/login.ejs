            <div id="signup">
                <div class="container">
                    <h1 id="signup-heading">Login</h1>
                    <form action="/login" method="post">
                    <div class="form-group">
                        <input class="form-control form-btns" type="text" name="username" placeholder="UserName">
                    </div>
                
                    <div class="form-group">
                        <input class="form-control form-btns" type="password" name="password" placeholder="Password">
                    </div>
                

                    <div class="form-group">
                        <button class="btn btn-lg btn-primary btn-block ">Submit!</button>
                    </div>
                </div>
            </div>
        </div>
    

