<% include ./partials/header %>
<div class="container">
    <div class="row">
        <div id="signup">
            <h1 id="signup-heading">Sign-Up</h1>
            <form action="/register" method="post">
                
                <div class="form-group">
                    <input class="form-control form-btns" type="text" name="username" placeholder="UserName">
                </div>
                
                <div class="form-group">
                     <input class="form-control form-btns" type="password" name="password" placeholder="Password">
                </div>
                

                <div class="form-group">
                     <button class="btn btn-lg btn-primary btn-block ">Submit!</button>
                </div>
                
            </form>
            
            <a href="/login">Already a Member? Login here!</a>
            
        </div>
        
        
        
    </div>
    
    
</div>
    
